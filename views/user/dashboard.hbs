<h1>Welcome to your personal Dashboard {{username}}</h1>

{{#if watchlist}}
<div>
    Your personal setlist:
    
    {{#each watchlist}}
    Festival ID: {{festival}}
    Bands:
    <ul>
    {{#each bands}}
        <li>{{this}}</li>
        {{/each}}
        </ul>
    {{/each}}
    
</div>
{{/if}}

{{#if notifications}}
<div>
    <h3>Notifications:</h3>
    {{#each notifications}}
    <div>
        <p>{{message}}</p>
        <p><i>{{date}} | {{type}}</i></p>
    </div>
    {{/each}}
</div>
{{/if}}